    "scripts": {
        "start": "webpack-dev-server --mode development --open",
        "build": "webpack --mode production",
        "test": "node scripts/test.js --env=jsdom",
        "prettier-all": "node_modules/.bin/prettier --config .prettierrc --write './src/**/*(*.css|*.js|*.scss)'"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,mjs}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "jest-canvas-mock",
            "jsdom-worker",
            "jest-gl"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^echarts-stat$": "<rootDir>/src/mock/echarts-statMock.js",
            "^react-handsontable$": "<rootDir>/src/mock/react-handsontableMock.js",
            "^.*webgl-heatmap": "<rootDir>/src/mock/webgl-heatmapMock.js",
            "worker-loader.*": "<rootDir>/src/mock/workerMock.js",
            "imports-loader.*": "<rootDir>/src/mock/importloaderMock.js"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/src/scripts/DISABLED_algorithms"
        ],
        "moduleFileExtensions": [
            "web.js",
            "mjs",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.2.2",
        "@babel/plugin-proposal-class-properties": "^7.3.0",
        "@babel/preset-env": "^7.3.1",
        "@babel/preset-react": "^7.0.0",
        "babel-loader": "^8.0.5",
        "concurrently": "^3.5.1",
        "css-loader": "^2.1.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "husky": "^1.3.1",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "jest-canvas-mock": "^1.0.3",
        "jest-gl": "^0.1.4",
        "jsdom-worker": "^0.1.0",
        "lint-staged": "^8.1.1",
        "mock-socket": "^7.1.0",
        "sinon": "^6.1.4",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.0",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.1.14"
    },
    "lint-staged": {
        "*.{js,css,scss,json}": [
            "prettier --config .prettierrc --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
