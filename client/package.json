{
    "name": "codex",
    "version": "0.1.0",
    "private": true,
    "homepage": ".",
    "dependencies": {
        "@mapbox/shelf-pack": "^3.2.0",
        "@trendmicro/react-dropdown": "^1.2.3",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react-hmre": "^1.1.1",
        "babel-preset-stage-2": "^6.24.1",
        "bootstrap": "^4.1.1",
        "browser-sync": "^2.26.3",
        "dotenv": "4.0.0",
        "echarts": "^4.1.0",
        "echarts-for-react": "^2.0.8",
        "echarts-gl": "^1.1.0",
        "hoist-non-react-statics": "^3.3.0",
        "immutable": "^3.8.2",
        "jest": "20.0.4",
        "object-assign": "4.1.1",
        "popper.js": "^1.14.3",
        "prettier": "^1.16.1",
        "promise": "8.0.1",
        "prop-types": "^15.6.2",
        "raf": "3.4.0",
        "react": "^16.3.2",
        "react-color": "^2.14.1",
        "react-contextmenu": "^2.9.2",
        "react-dom": "^16.3.2",
        "react-handsontable": "^0.3.1",
        "react-immutable-proptypes": "^2.1.0",
        "react-markdown": "^3.3.0",
        "react-redux": "^5.0.7",
        "react-sortable-hoc": "^0.6.8",
        "reactcss": "^1.2.3",
        "reactstrap": "^5.0.0",
        "redux": "^4.0.0",
        "redux-logger": "^3.0.6",
        "redux-thunk": "^2.3.0",
        "style-loader": "0.19.0",
        "styled-components": "^3.3.3",
        "ts-react-toolbox": "^0.2.2",
        "webpack-hot-middleware": "^2.24.3",
        "whatwg-fetch": "2.0.3",
        "worker-loader": "^2.0.0"
    },
    "scripts": {
        "start": "npm run open:src",
        "open:src": "babel-node --max-old-space-size=4076 scripts/devServer.js",
        "build": "webpack --mode production",
        "test": "node scripts/test.js --env=jsdom",
        "prettier-all": "node_modules/.bin/prettier --config .prettierrc --write './src/**/*(*.css|*.js|*.scss)'"
    },
    "jest": {
        "collectCoverageFrom": [
            "src/**/*.{js,jsx,mjs}"
        ],
        "setupFiles": [
            "<rootDir>/config/polyfills.js",
            "jest-canvas-mock",
            "jsdom-worker",
            "jest-gl"
        ],
        "testMatch": [
            "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
            "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
        ],
        "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.js",
        "testEnvironment": "node",
        "testURL": "http://localhost",
        "transform": {
            "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
            "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
            "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
        },
        "transformIgnorePatterns": [
            "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
        ],
        "moduleNameMapper": {
            "^react-native$": "react-native-web",
            "^echarts-stat$": "<rootDir>/src/mock/echarts-statMock.js",
            "^react-handsontable$": "<rootDir>/src/mock/react-handsontableMock.js",
            "^.*webgl-heatmap": "<rootDir>/src/mock/webgl-heatmapMock.js",
            "worker-loader.*": "<rootDir>/src/mock/workerMock.js",
            "imports-loader.*": "<rootDir>/src/mock/importloaderMock.js"
        },
        "testPathIgnorePatterns": [
            "<rootDir>/src/scripts/DISABLED_algorithms"
        ],
        "moduleFileExtensions": [
            "web.js",
            "mjs",
            "js",
            "json",
            "web.jsx",
            "jsx",
            "node"
        ]
    },
    "devDependencies": {
        "babel-preset-react": "^6.24.1",
        "concurrently": "^3.5.1",
        "css-loader": "^2.1.0",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "file-loader": "^3.0.1",
        "html-webpack-plugin": "^3.2.0",
        "husky": "^1.3.1",
        "jest-canvas-mock": "^1.0.3",
        "jest-gl": "^0.1.4",
        "jsdom-worker": "^0.1.0",
        "lint-staged": "^8.1.1",
        "mock-socket": "^7.1.0",
        "node-sass": "^4.11.0",
        "sass-loader": "^7.1.0",
        "sinon": "^6.1.4",
        "ts-loader": "^5.3.3",
        "typescript": "^3.3.3",
        "url-loader": "^1.1.2",
        "webpack": "^4.29.6",
        "webpack-cli": "^3.2.1",
        "webpack-dev-server": "^3.1.14",
        "@atlaskit/button": "^10.1.3",
        "@atlaskit/checkbox": "^6.0.0",
        "@atlaskit/field-text": "^8.0.0",
        "@atlaskit/single-select": "^7.0.0"
    },
    "lint-staged": {
        "*.{js,css,scss,json}": [
            "prettier --config .prettierrc --write",
            "git add"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    }
}
